# speech_share - ARC Script

Servo(D4, 120)  # Left arm forward
Servo(D5, 100)
Servo(D6, 40)   # Palm open

Servo(D7, 50)   # Right arm forward
Servo(D8, 100)
Servo(D9, 150)  # Palm open
Sleep(600)

# Reset
Servo(D4, 150)
Servo(D5, 120)
Servo(D6, 90)
Servo(D7, 30)
Servo(D8, 60)
Servo(D9, 90)
Sleep(400)